h=parseInt($("#same_numberOfGames").val()),u=parseInt($("#same_initialMultiple").val()),c;if(isNaN(h)||isNaN(u)){warningMessage("期数与倍数请输入整数数字");return}if(viewModel.lotteryTickets().length==0){warningMessage("请先加注再追注");return}viewModel.isScheduleOpen(!0);for(var o=recentissues.slice(0,numberOfSchedulableIssue),n=0,s=[],f=viewModel.totalSum()*u,t=0,i=0,r=0,n=0;n<o.length;n++)n<h+e&&n>=e?(t+=f,viewModel.lotteryTickets().length==1?(c=viewModel.lotteryTickets()[0].topPrize()*viewModel.lotteryTickets()[0].multiple(),i=c*u-t,r=i/t*100,i=math.round(i,DecimalPrecision),r=math.round(r,DecimalPrecision)):(i="",r=""),f=math.round(f,DecimalPrecision),t=math.round(t,DecimalPrecision),s.push(new SchedulePreview(o[n].GameSerialNumber,u,f,t,i,r,!0))):s.push(new SchedulePreview(o[n].GameSerialNumber,"",0,0,"","",!1));viewModel.schedulepreviews(s);viewModel.UpdateScheduleData();$("#same_startFrom").val(e)}function double_generatePlan(){var e=$("#double_startFrom").length>0?parseInt($("#double_startFrom").val()):0,h=parseInt($("#double_numberOfGames").val()),c=parseInt($("#double_interval").val()),l=parseInt($("#double_initialMultiple").val()),a=parseInt($("#double_multiple").val()),v;if(isNaN(h)||isNaN(c)||isNaN(l)||isNaN(a)){warningMessage("期数与倍数请输入整数数字");return}if(viewModel.lotteryTickets().length==0){warningMessage("请先加注再追注");return}viewModel.isScheduleOpen(!0);for(var o=recentissues.slice(0,numberOfSchedulableIssue),n=0,s=[],r=0,u=0,t=0,i=0,f=0,n=0;n<o.length;n++)if(n<h+e&&n>=e){if(r=a*Math.pow(l,Math.floor((n-e)/c)),u=viewModel.totalSum()*r,r>=1e8){warningMessage("请调低翻倍倍数，或调高翻倍间隔期数");return}t+=u;viewModel.lotteryTickets().length==1?(v=viewModel.lotteryTickets()[0].topPrize()*viewModel.lotteryTickets()[0].multiple(),i=v*r-t,f=i/t*100,i=math.round(i,DecimalPrecision),f=math.round(f,DecimalPrecision)):(i="",f="");u=math.round(u,DecimalPrecision);t=math.round(t,DecimalPrecision);s.push(new SchedulePreview(o[n].GameSerialNumber,r,u,t,i,f,!0))}else s.push(new SchedulePreview(o[n].GameSerialNumber,"",0,0,"","",!1));viewModel.schedulepreviews(s);viewModel.UpdateScheduleData();$("#double_startFrom").val(e)}function calculate_lowest_multiple_bet(n,t,i,r,u){var f=0;return f=n==0?(r-t)/t>u?i:0:i+n*(u+1)/(r-t-u*t),f>=1e8&&(f=0),Math.ceil(f)}function profee_generatePlan(){var e=$("#profee_startFrom").length>0?parseInt($("#profee_startFrom").val()):0,c=parseInt($("#profee_numberOfGames").val()),l=parseInt($("#profee_initialMultiple").val()),a=parseInt($("#profee_lowestPercent").val())/100,h;if(isNaN(c)||isNaN(a)||isNaN(l)){warningMessage("期数、倍数与收益率请输入整数数字");return}if(viewModel.lotteryTickets().length==0){warningMessage("请先加注再追注");return}viewModel.isScheduleOpen(!0);for(var o=recentissues.slice(0,numberOfSchedulableIssue),n=0,s=[],i=0,r=0,t=0,u=0,f=0,n=0;n<o.length;n++){if(h=viewModel.lotteryTickets()[0].topPrize()*viewModel.lotteryTickets()[0].multiple(),i=calculate_lowest_multiple_bet(t,viewModel.totalSum(),l,h,a),r=viewModel.totalSum()*i,i<1){warningMessage("请调低最低收益率");return}n<c+e&&n>=e?(t+=r,u=h*i-t,f=u/t*100,r=math.round(r,DecimalPrecision),t=math.round(t,DecimalPrecision),u=math.round(u,DecimalPrecision),f=math.round(f,DecimalPrecision),s.push(new SchedulePreview(o[n].GameSerialNumber,i,r,t,u,f,!0))):s.push(new SchedulePreview(o[n].GameSerialNumber,"",0,0,"","",!1))}viewModel.schedulepreviews(s);viewModel.UpdateScheduleData();$("#profee_startFrom").val(e)}function ClickScheduleButton(){var r=viewModel.isScheduleOpen();if(r)viewModel.isScheduleOpen(!1);else{if(viewModel.lotteryTickets().length==0)return warningMessage("请先加注再追注"),!1;viewModel.isScheduleOpen(!0);for(var t=recentissues.slice(0,numberOfSchedulableIssue),n=0,i=[],n=0;n<t.length;n++)i.push(new SchedulePreview(t[n].GameSerialNumber,"",0,0,"","",!1));viewModel.schedulepreviews(i);viewModel.UpdateScheduleData();$("#sameTab a").click();$("#scheduleModal").modal("show")}return!0}function CleanScheduleTable(){var r=viewModel.isScheduleOpen();if(r){if(viewModel.lotteryTickets().length==0){viewMod