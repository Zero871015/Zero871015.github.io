iftMMCWinningNumber(n);$("#walletamount",getTopWindow().document).text(t)},e+1500)}function continuourun(){service.BetConfirm()}function MMCifcontinuourun(){if(viewModel.lotteryTickets().length==0){warningMessage("请至少选择一注");return}service.BetConfirm()}function MMCwindivhide(){var n=getTopWindow().document;$("#mmcendDiv",n).hide();$("#mmclosetxt",n).hide();$("#mmcwintxt",n).hide();$("#mmcwinmoneytxt",n).hide();$("#mmcnomoneytxt",n).hide();$("#moneytxt",n).hide()}function mmcreset(){slotisOK=!0;totalruncount=0;runcount=1;WinningPrize=0;TotalWinningPrize=0;MMCcheckbox=!1;clearTimeout(MMCtimeout);PushSingleTempBets();$("#sloting").hide()}function PushSingleTempBets(){SingleTempBets.length>0&&($.each(SingleTempBets,function(n,t){UnshiftBetRecord(t)}),SingleTempBets=[])}function MMCViewwintext(){var n=getTopWindow().document;$("#mmcwinmoneytxt",n).text(TotalWinningPrize);$("#moneytxt",n).text("钱包余额不足,请重新选號");$("#mmcwinmoneytxt",n).show();$("#moneytxt",n).show();$("#mmcwintxt",n).show();$(".mmcagain",n).hide();$("#mmcendDiv",n).show()}function MMCViewlosetext(){var n=getTopWindow().document;$("#mmcnomoneytxt",n).text("钱包余额不足,请重新选號");$("#mmcnomoneytxt",n).show();$("#mmclosetxt",n).show();$(".mmcagain",n).hide();$("#mmcendDiv",n).show()}function MMCViewwinendtxt(){var n=getTopWindow().document;$("#mmcwinmoneytxt",n).text(TotalWinningPrize);$("#mmcwinmoneytxt",n).show();$("#mmcwintxt",n).show()}function MMCcontinuoues(){var n=getTopWindow().document;MMCcheckbox==!0&&WinningPrize!=0&&totalruncount!=0?mmcend():totalruncount!=0&&(runcount!=totalruncount&&totalruncount!=0?(runcount++,WinningPrize!=0&&($("#mmcwinmoneySpan",n).text(WinningPrize),$("#mmcwinmoneyDiv",n).show(),setTimeout(function(){$("#mmcwinmoneyDiv",n).hide();$("#mmcrunSpan",n).text(runcount)},2e3)),setTimeout(function(){slotisOK=!0;continuourun()},2500)):mmcend())}function mmcend(){var n=getTopWindow().document;(TotalWinningPrize!=0?MMCViewwinendtxt():$("#mmclosetxt",n).show(),slotisOK!=!0)&&($(".mmcagain",n).show