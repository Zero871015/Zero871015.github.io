Ccheckbox").prop("checked")?!0:!1,totalruncount=$("#run").val(),MMCwindivhide(),MMCifcontinuourun(),!1});$(".mmcafresh",n).unbind("click").click(function(){MMCwindivhide();viewModel.lotteryTickets([]);ClearAllPanelData()});window.addEventListener?window.addEventListener("unload",function(){$(".mmcagain",n).unbind("click");$(".mmcafresh",n).unbind("click")}):window.attachEvent("unload",function(){$(".mmcagain",n).unbind("click");$(".mmcafresh",n).unbind("click")});t=getTopWindow();$("#scheduleModal").on("show.bs.modal",function(){if(n.querySelector("iframe")){var i=t.scrollY;$("#scheduleModal").css("top",i<0?0:i)}}).on("hide.bs.modal",ClickScheduleButton);$("#bettype_toggle").change(function(){$(".regular-bettype, .input-bettype").hide();var n=$("#bettype_toggle:checked").length>0;n?($(".input-bettype").show(),isLocalStorageAvailable&&(window.localStorage["LastUseBetType_"+LottoBetTypeName]="input"),SetLastInputBetType()):($(".bet-type-button.current").trigger("click"),$(".regular-bettype").show(),isLocalStorageAvailable&&(window.localStorage["LastUseBetType_"+LottoBetTypeName]="regular"),SetLastSelectButton())});$("#intput-bet-type-items").change(function(){BetTypeId=$(this.options[this.selectedIndex]).attr("id");viewModel.positions([]);BetTypeMethod[BetTypeId].Panel();isLocalStorageAvailable&&(window.localStorage["LottoInput_LastSeletedBetType_"+LottoBetTypeName]=BetTypeId);$(".roadPostionSelect").length>0&&($(".roadPostionSelect").get(0).options[0].selected=!0,$(".roadPostionSelect").children().each(function(n){var t=$(this).data("betname");t==BetTypeId&&($(".roadPostionSelect").get(0).options[n].selected=!0)}),$(".roadPostionSelect").trigger("change"))});SetupPanel();SetLastSelectButton();$("#Bet").show()}function HideBetType(){var t,n;for(SetBetTypeState(),$(".bet-type-button").addClass("hide"),$(".bet-type-button-group").addClass("hide"),$(".bettype-button-list").addClass("hide"),t=$(".bet-type-button"),n=0;n<t.length;n++)LottoBetType[$(t[n]).attr("id")]&&Lotto