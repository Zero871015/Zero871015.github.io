function crc16ccitt(n){for(var t=65535,r,i=0;i<n.length;i++){if(c=n.charCodeAt(i),c>255)throw new RangeError;r=(c^t>>8)&255;t=crcTable[r]^t<<8}return(t^0)&65535}function ProcessTicket(n,t){var u=viewModel.lotteryTickets(),r,i,e,f;if(n.LotteryGameID=lotteryGameId,n.SerialNumber=currentSerialNumber,n.Guid=GenerateGuid(),n.IsLoginByWeChat=isLoginByWeChat(),typeof t!="undefined"){if(!currentSerialNumber){weChatBetChat.addSystemInfo("暂停下注",!1,!1);return}n.BetMode=BetMode.TEXT;n.Bets=WeChatBetText.processText(t)}else for(r=0;r<u.length;r++)i=new BetData,i.BetTypeCode=parseInt(u[r].betType()),e=BetTypeMethod[u[r].betTypeId].NumbersCompress,f=u[r].number(),e&&canCompress()&&(i.Number=e(f),i.CompressedNumberCrc=crc16ccitt(f).toString(16),i.Number!=null&&(i.IsCompressed=!0)),i.IsCompressed==!1&&(i.Number=f),i.Position=u[r].position(),i.Unit=parseFloat(u[r].unit()),i.Multiple=parseInt(u[r].multiple()),i.ReturnRate=u[r].returnRate()==0?0:1,i.NoCommission=typeof u[r].noCommission!="undefined"&&typeof u[r].noCommission()=="boolean"?u[r].noCommission():!1,n.Bets.push(i)}function Service(){this.betTimeToken=(new Date).getTime();this.baseAjaxOptions={ScheduleConfirmOptions:{type:"POST",dataType:"json",contentType:"application/json; charset=utf-8"},BetConfirmOptions:{type:"POST",dataType:"json",contentType:"application/json; charset=utf-8"},GetBetRecordsOptions:{type:"POST",url:CombinationURL("/BetRecord/QuickSearch")},GetScheduleRecordsOptions:{type:"POST",url:CombinationURL("/ScheduleRecord/Search")},GetProfitRecordsOptions:{type:"POST",url:CombinationURL("/ProfitReport/Query")},GetCashFlowRecordsOptions:{type:"POST",url:"/CashFlowRecord/Search"},GetBetProposalsOptions:{type:"POST",url:CombinationURL("/BetProposal/GetAllBetProposal")}};this.extendAjaxOptions=function(n){$.extend(!0,this.baseAjaxOptions,n)};this.ScheduleConfirm=function(){var r=(new Date).getTime(),f=r-this.betTimeToken,n,i,t,u;if((this.betTimeToken=r,!(f<1e3))&&(n=new BetDataModel,ProcessTicket(n),n.Bets!==null&&!(