function SetInputBetType(){switch(lotteryCategoryId){case LottoCategory.SHISHICAI:case LottoCategory.FENFENCAI:case LottoCategory.MIAOMIAOCAI:case LottoCategory.KENO:InputBetTypeInit();SetShishiCaiInputBetType();break;case LottoCategory.ELEVENXFIVE:case LottoCategory.FENFENELEVENXFIVE:case LottoCategory.MIAOMIAOELEVENXFIVE:InputBetTypeInit();SetElevenFiveInputBetType();break;case LottoCategory.KUAI3:case LottoCategory.FENFENKUAI3:case LottoCategory.MIAOMIAOKUAI3:InputBetTypeInit();SetKUAI3InputBetType();break;case LottoCategory.HAPPY8:case LottoCategory.FENFENHAPPY8:InputBetTypeInit();SetHappy8InputBetType();break;case LottoCategory.KUAIKAI:case LottoCategory.FENFENKUAIKAI:InputBetTypeInit();SetKUAIKAIInputBetType();break;case LottoCategory.KUAILE10:case LottoCategory.FENFENKUAILE10:InputBetTypeInit();SetKUAILE10InputBetType()}}function InputBetTypeInit(){$(".input-bettype-confirm").unbind("click").click(function(n){var t,i;return(n.preventDefault(),viewModel.singleCount()<=0)?(warningMessage("请选择号码"),!1):(AddInputTicket(),t=CheckInputBetTypeState(),t.length==0?(i="已选择 "+viewModel.singleCount()+" 注，总金额 "+viewModel.singleSum()+" 元<br>是否确认投注 ?",confirmMessage(i,function(){betConfirm()})):(warningMessage('"'+t.join("、")+'"暂时无法投注，请重新选择。'),viewModel.lotteryTickets([])),!1)})}function SetLastInputBetType(){$("#intput-bet-type-items .input-bettype-button").removeProp("selected");isLocalStorageAvailable&&$("#"+window.localStorage["LottoInput_LastSeletedBetType_"+LottoBetTypeName]+":not(.hide)").length>0?$("#"+window.localStorage["LottoInput_LastSeletedBetType_"+LottoBetTypeName]).prop("selected",!0):$("#intput-bet-type-items .input-bettype-button:not(.hide):first").prop("selected",!0);$("#intput-bet-type-items").trigger("change")}function HideInputBetType(){$(".input-bettype-button").addClass("hide");$(".input-bettype-button").each(function(){var n=$(this).attr("id");LottoBetType[n]&&LottoBetType[n].State&&$(this).removeClass("hide")})}function InputBetTypeFastModeChange(){