.hide();$("#TwoSidePlateIntegrationPanel").hide();$("#Number8IntegrationPanel").hide();$("#Number7IntegrationPanel").hide();$("#Number6IntegrationPanel").hide();$("#Number5IntegrationPanel").hide();$("#Number4IntegrationPanel").hide();$("#Number3IntegrationPanel").hide();$("#Number2IntegrationPanel").hide();$("#Number1IntegrationPanel").hide();$("#BigSmallDicePanel").hide();$("#Happy8IntegrationPanel").hide();$("#PK10IntegrationPanel").hide();$("#pos1pos2sumIntegrationPanel").hide();$("#ArbitraryPositionIntegrationPanel").hide();$("#NumberPositionMatchIntegrationPanel").hide();$("#inputBetType").removeClass("active");$("#regularBetType").addClass("active");$(".roadTrendMenu").hide()}function InputBetTypePanel(){HideAllInputBetTypePanel();ClearAllPanelData();$("#roadTrend-panel").hide();$(".regular-bettype, .table-bettype").hide();$(".input-bettype").show();$("#regularBetType").removeClass("active");$("#inputBetType").addClass("active")}function SSCIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#SSCIntegrationPanel").show();RoadTrendPanel()}function LongHuHeIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#LongHuHeIntegrationPanel").show();RoadTrendPanel()}function GuessMatchNumberIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#GuessMatchNumberIntegrationPanel").show()}function TwoSidePlateIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#TwoSidePlateIntegrationPanel").show()}function Number5IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number5IntegrationPanel").show();RoadTrendPanel()}function Number4IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number4IntegrationPanel").show();RoadTrendPanel()}function Number3IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number3IntegrationPanel").show();RoadTrendPanel()}function Number2IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number2IntegrationPanel").show();RoadTrendPanel()}function Number1IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number1IntegrationPanel").show();RoadTrendPanel()}function BigSmallDicePanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#BigSmallDicePanel").show();RoadTrendPanel()}function Happy8IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Happy8IntegrationPanel").show();RoadTrendPanel()}function PK10IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#PK10IntegrationPanel").show();RoadTrendPanel()}function NumberPositionMatchIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#NumberPositionMatchIntegrationPanel").show();RoadTrendPanel()}function pos1pos2sumIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#pos1pos2sumIntegrationPanel").show()}function ArbitraryPositionIntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#ArbitraryPositionIntegrationPanel").show()}function Number6IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number6IntegrationPanel").show();RoadTrendPanel()}function Number7IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number7IntegrationPanel").show();RoadTrendPanel()}function Number8IntegrationPanel(){InputBetTypePanel();$(".roadTrendMenu").show();$("#Number8IntegrationPanel").show();RoadTrendPanel()}function RoadTrendInit(){if(!isRoadTrendInitialize){var n=0;$(".horiDrag").on({mousemove:function(t){var i=t.pageX-this.offsetLeft;n&&(this.scrollLeft=this.scroll_X+n-i)},mousedown:function(t){this.scroll_X=this.scrollLeft;n=t.pageX-this.offsetLeft},mouseleave:function(){n=0},mouseup:function(){n=0},touchmove:function(t){var i=t.originalEvent.touches[0].pageX-this.offsetLeft;n&&(this.scrollLeft=this.scroll_X+n-i)},touchstart:function(t){this.scroll_X=this.scrollLeft;n=t.originalEvent.touches[0].pageX-this.offsetLeft},touchend:function(){n=0}});$(".roadTypeSelect").on({click:function(){$(this).hasClass("active")||($(".roadTypeSelect").removeClass("active"),$(this).addClass("active"));var n="";$(".roadPostionSelect").val()=="#roadPosition"&&$(".roadPostionSelect :selected").is("[data-position]")&&(n=$(".roadPostionSelect :selected").data("position"));$("#roadTrendContent .tab-pane").hide();$($(this).attr("data-value")+n).show();RoadTrendPushCallBack()}});$(".roadPostionSelect").on({change:function(){$("#roadTypeSelects ul").hide();$($(this).val()).show();$($(this).val()).find(".roadTypeSelect.active").length>0?$($(this).val()).find(".roadTypeSelect.active").trigger("click"):$($(this).val()).find(".roadTypeSelect:first").trigger("click");RoadTrendPushCallBack()}}).trigger("change");RoadTrend.setOptions({pushCallback:RoadTrendPushCallBack,initStartCallback:RoadTrendInitStartCallBack,initCompleteCallback:RoadTrendInitCompleteCallBack});switch(lotteryCategoryId){case LottoCategory.SHISHICAI:case LottoCategory.FENFENCAI:case LottoCategory.KENO:RoadTrend.addAllRoads(SetShishiCaiRoadTrend());RoadTrend.init();break;case LottoCategory.ELEVENXFIVE:case LottoCategory.FENFENELEVENXFIVE:RoadTrend.addAllRoads(SetElevenXFiveRoadTrend());RoadTrend.init();break;case LottoCategory.KUAIKAI:case LottoCategory.FENFENKUAIKAI:RoadTrend.addAllRoads(SetKuaiKaiRoadTrend());RoadTrend.init();break;case LottoCategory.HAPPY8:case LottoCategory.FENFENHAPPY8:RoadTrend.addAllRoads(SetHAPPY8RoadTrend());RoadTrend.init();break;case LottoCategory.KUAILE10:case LottoCategory.FENFENKUAILE10:RoadTrend.addAllRoads(SetKueile10RoadTrend());RoadTrend.init()}isRoadTrendInitialize=!0}}function RoadTrendReInit(){isRoadTrendInitialize&&RoadTrend.init()}function RoadTrendPush(n,t,i,r){isRoadTrendInitialize&&RoadTrend.push(n,t,i,r)}function RoadTrendUpdate(n,t,i,r){isRoadTrendInitialize&&RoadTrend.update(n,t,i,r)}function RoadTrendPushCallBack(){$(".roadScroll").each(function(n,t){var i=$("table",$(t)).outerWidth()-$(t).outerWidth();i>0&&$(t).scrollLeft(i)})}function RoadTrendInitStartCallBack(){$(".ludan-loading").show()}function RoadTrendInitCompleteCallBack(){$(".ludan-loading").hide()}function RoadTrendPanel(){RoadTrendInit();$("#roadTrend-panel").show();RoadTrendPushCallBack()}function SetShishiCaiRoadTrend(){var s={N0:'<div class="redbg">0<\/div>',N1:'<div class="bluebg">1<\/div>',N2:'<div class="redbg">2<\/div>',N3:'<div class="bluebg">3<\/div>',N4:'<div class="redbg">4<\/div>',N5:'<div class="bluebg">5<\/div>',N6:'<div class="redbg">6<\/div>',N7:'<div class="bluebg">7<\/div>',N8:'<div class="redbg">8<\/div>',N9:'<div class="bluebg">9<\/div>'},e={BIG:'<div class="redbg"><\/div>',SMALL:'<div class="bluebg"><\/div>',EVEN:'<div class="redbg"><\/div>',ODD:'<div class="bluebg"><\/div>'},o={BIG:'<div class="redbg">大<\/div>',SMALL:'<div class="bluebg">小<\/div>',EVEN:'<div class="redbg">双<\/div>',ODD:'<div class="bluebg">单<\/div>'},r={RED:'<div class="redbordercircle"><\/div>',BLUE:'<div class="bluebordercircle"><\/div>'},u={RED:'<div class="redbg"><\/div>',BLUE:'<div class="bluebg"><\/div>'},f={RED:'<div class="redline">/<\/div>',BLUE:'<div class="blueline">/<\/div>'},h={LONG:'<div class="redbg"><\/div>',HU:'<div class="bluebg"><\/div>',HE:'<div class="greenbg"><\/div>'},c={LONG:'<div class="redbg">龙<\/div>',HU:'<div class="bluebg">虎<\/div>',HE:'<div class="greenbg">和<\/div>'},n=6,i=45,t=10;return isMobileBrowser()&&(i=13,t=6),[{name:"BigRoad_P0",row:n,column:i,position:0,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s},{name:"BigRoad_P1",row:n,column:i,position:1,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s},{name:"BigRoad_P2",row:n,column:i,position:2,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s},{name:"BigRoad_P3",row:n,column:i,position:3,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s},{name:"BigRoad_P4",row:n,column:i,position:4,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s},{name:"NormalRoad_P0",row:n,column:t,position:0,dotDecider:ShiShiCaiRoadAlgorithm.numberDotCode,dotStyle:s,specialRoadType:RoadTrend.SPEC_RO