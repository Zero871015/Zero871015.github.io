function popupiFrameWarningMessage(n,t){if($("#warning-dialog").is(":visible")){$("#warning-dialog").html(n);var i=(new Date).valueOf()+3e3;$("#confirmBtn").countdown(i.toString()).off("finish.countdown").on("finish.countdown",function(){$("#warning-dialog").dialog("close");t!=null&&location.replace(t)});return}$("#warning-dialog").html(n);$("#warning-dialog").dialog({resizable:!1,modal:!0,title:"温馨提示",buttons:[{text:"确认 (3)",id:"confirmBtn",click:function(){$(this).dialog("close");t!=null&&location.replace(t)}}],open:function(){var n=(new Date).valueOf()+3e3;$("#confirmBtn").countdown(n.toString()).on("update.countdown",function(n){$("#confirmBtn span").text(n.strftime("确认  (%-S)"))}).on("finish.countdown",function(){$("#warning-dialog").dialog("close");t!=null&&location.replace(t)})},close:function(){$(this).dialog("destroy");t!=null&&location.replace(t)}})}function warningMessage(n,t){function u(){i.modal("hide");i.css("display","none");$(".close, .confirmBtn",i).unbind("click");t!=null&&location.replace(t)}var r=getTopWindow(),e=r.document,i=$("#warning-dialog",e),o=i.find(".modal-dialog"),s=($(r).height()-o.height())/4,f;i.css({"margin-top":s+"px"});$(".content",i).html(n);f=(new Date).valueOf()+3e3;$(".confirmBtn",i).countdown(f.toString()).on("update.countdown",function(n){$(".confirmBtn",i).text(n.strftime("确认(%-S)"))}).on("finish.countdown",u);$(".close, .confirmBtn",i).click(u);i.modal("show")}function expireMessage(n){function r(){t.modal("hide");$(".close, .confirmBtn",t).unbind("click")}var i=getTopWindow(),f=i.document,t=$("#expire-dialog",f),e=t.find(".modal-dialog"),o=($(i).height()-e.height())/4,u;t.css({"margin-top":o+"px"});$(".content",t).html(n);u=(new Date).valueOf()+3e3;$(".confirmBtn",t).countdown(u.toString()).on("update.countdown",function(n){$(".confirmBtn",t).text(n.strftime("确认(%-S)"))}).on("finish.countdown",r);$(".close, .confirmBtn",t).click(r);t.modal("show")}function confirmMessage(n,t,i){function f(){r.modal("hide");$(".close, .confirmBtn, .cancelBtn",r).unbind("click")}function h(){t(i);f()}var u=getTopWindow(),e=u.document,r=$("#confirm-dialog",e),o=r.find(".modal-dialog"),s=($(u).height()-o.height())/4;r.css({"margin-top":s+"px"});$(".content",r).html(n);$(".confirmBtn",r).click(h);$(".close, .cancelBtn",r).click(f);r.modal("show")}function iframeMessage(n,t,i){function f(){r.modal("hide");$(".close, .confirmBtn",r).unbind("click");t(i)}var u=getTopWindow(),o=u.document,r=$("#iframe-dialog",o),s=r.find(".modal-dialog"),h=($(u).height()-s.height())/4,e;r.css({"margin-top":h+"px"});$(".content",r).html(n);e=(new Date).valueOf()+3e3;$(".confirmBtn",r).countdown(e.toString()).on("update.countdown",function(n){$(".confirmBtn",r).text(n.strftime("确认(%-S)"))}).on("finish.countdown",f);$(".close, .confirmBtn",r).click(f);r.modal("show")}function withdrawWarningMessage(n,t){function u(){i.modal("hide");$("#create").attr("disabled",!1);$(".close, .confirmBtn, .cancelBtn",i).unbind("click")}function s(){var n=$("#form");n.valid()?n.submit():$("#create").attr("disabled",!1);u()}var r=getTopWindow(),f=r.document,i=$("#confirm-dialog",f),e=i.find(".modal-dialog"),o=($(r).height()-e.height())/4;i.css({"margin-top":o+"px"});$(".content",i).html("每日"+n+"~"+t+"間<p>因为上游渠道问题会有延迟到帐的情形<\/p>");$(".confirmBtn",i).click(s);$(".close, .cancelBtn",i).click(u);i.modal("show")}